{
  "name": "vue-cli-overall-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "yarn serve",
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "preview": "node ./commands/preview.js",
    "deploy": "bash ./commands/deploy.sh",
    "clear-commit-msg": "bash ./commands/clear-commit-msg.sh",
    "analyz": "NODE_ENV=production  npm_config_report=true npm run build",
    "lint": "vue-cli-service lint",
    "format": "prettier-eslint --write \"src/**/*.js\" \"src/**/*.vue\""
  },
  "dependencies": {
    "axios": "^0.18.0",
    "dayjs": "^1.6.6",
    "js-cookie": "^2.2.0",
    "q": "^1.5.1",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.11",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.11",
    "@vue/cli-service": "^3.0.0-beta.11",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "file-loader": "^1.1.11",
    "node-sass": "^4.9.0",
    "prerender-spa-plugin": "^3.2.1",
    "prettier-eslint-cli": "^4.7.1",
    "sass-loader": "^7.0.1",
    "svg-sprite-loader": "^3.8.0",
    "svgo-loader": "1.1.0",
    "vue-svg-loader": "^0.5.0",
    "vue-template-compiler": "^2.5.16",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "plugin:prettier/recommended",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
